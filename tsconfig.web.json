{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": ["DOM", "DOM.Iterable", "ES2020"],
    "strict": false,
    "noEmitOnError": true,
    "rootDir": "public",
    "outDir": "public",
    "skipLibCheck": true,
    "moduleDetection": "force",
    "verbatimModuleSyntax": false
  },
  "include": ["public/**/*.ts"],
  "exclude": []
}
